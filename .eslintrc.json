{
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "plugins": ["prettier"],
    "env": {
        "browser": true,
        "node": false,
        "commonjs": false,
        "es2020": true,
        "jquery": true
    },
    "parserOptions": {
        "ecmaVersion": 2019
    },
    "rules": {
        "prettier/prettier": "error"
    }
}