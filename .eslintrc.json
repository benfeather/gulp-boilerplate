{
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "plugins": ["prettier"],
    "env": {
        "browser": true,
        "node": true,
        "commonjs": false,
        "es6": true,
        "jquery": false
    },
    "parserOptions": {
        "ecmaVersion": 2019
    },
    "rules": {
        "prettier/prettier": "error"
    }
}